            <div class="title">
              <h4>معلومات الحجز</h4>
            </div>
            <div class="field-row" style="grid-template-columns:11% 35%;">
              <div class="field-cell">
                <label for="contractNum">رقم العقد</label>
                <label id="contractNum" name="contractNum" class="pdr10">--.--.--</label>

              </div>


              <div class="field-cell">
                <label for="bookingSrc">مصدر الحجز</label>
                <select id="bookingSrc" name="bookingSrc" form="frmCheckIn" class="input100 fontDroid combobox">

                  <% if(settings[0]){settings[0].bookingSrc.forEach(type => { %>
                    <option value=<%=type %> >
                      <%= type  %>
                    </option>
                    <% })} %>

                </select>
              </div>
            </div>
            <!-- Allow form submission with keyboard without duplicating the dialog button -->
            <!-- <input type="submit" tabindex="-1" style="position:absolute; top:-1000px"> -->
          </fieldset>

          <fieldset>
            <div class="title">
              <h4>الفترة</h4>
            </div>

            <div class="field-row pdt20" style="grid-template-columns:20% 40% 10%;">
              <div class="field-cell">
                <table class="wid100">
                  <tr>
                    <td>
                      <label class="" for="bookingType">نوع الايجار</label>
                    </td>
                    <td>
                      <select id="bookingType" name="bookingType" form="frmCheckIn" class="input100 fontDroid combobox">
                        <% if(settings[0]){settings[0].bookingTypes.forEach(type => { %>
                          <option value=<%=type %> >
                            <%= type  %>
                          </option>
                          <% })} %>
                      </select>
                  </tr>
                  </td>
                  <tr>
                    <td>
                      <label class="" for="calander">التقويم</label>
                    </td>
                    <td>
                      <select id="" name="calander" form="frmCheckIn" class="input100 fontDroid combobox">
                        <option value="ميلادي">ميلادي</option>
                        <option value="هجري">هجري</option>
                      </select>
                    </td>
                  </tr>
                </table>
              </div>


              <div class="field-cell">
                <table class="wid100">
                  <tr>
                    <td>
                      <label class="">ميلادي</label>
                    </td>
                    <td style="position:relative;">
                      <label style="position:absolute;top:-16px;">من</label>
                      <div class="sandbox-container">
                        <input type="date" class="input100 fontDroid combobox" name="checkIn">
                      </div>
                      
                    
                    </td>
                    <td style="position:relative;">
                      <label style="position:absolute;top:-16px">إلى</label>
                      <div class="sandbox-container">
                        <input type="date" class="input100 fontDroid combobox" name="checkOut">
                      </div>
                    </td>
                    <td class="pdr20" style="position:relative;">
                      <label style="position:absolute;top:-16px">الوقت</label>
                      <label class="">00:00</label>
                    </td>
                  </tr>
                  <tr>
                    <td>
                      <label class="">هجري</label>
                    </td>
                    <td>
                      <div id="sandbox-container">
                        <input type="date" class="input100 fontDroid combobox">
                      </div>
                    </td>
                    <td>
