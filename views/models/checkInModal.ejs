                        <input type="date" class="input100 fontDroid combobox">
                      </div>
                    </td>
                    <td class="pdr20">
                      <label class="">00:00</label>
                    </td>
                  </tr>
                </table>
              </div>
              <div class="field-cell">
                <label class="fltr pdl20 pdt5 pdr20" for="input_daysNum">عدد الأيام</label>

                <div class="wrap-input100 validate-input fontDroid" data-validate="الاسم الاب مطلوب" style="display:table-cell">
                  <input id="input_daysNum" class="input100 fontDroid combobox" type="text" name="daysNum" form="frmCheckIn" value="1">
                  <span class="focus-input100"></span>
              </div>
              </div>







            </div>
            <!-- Allow form submission with keyboard without duplicating the dialog button -->
          </fieldset>
          <fieldset>
            <div class="title">
