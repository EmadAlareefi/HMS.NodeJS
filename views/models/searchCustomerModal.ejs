            <div class="lds-ellipsis hidden"><div></div><div></div><div></div><div></div></div>

            <div class="modal-body">

                <div class="field-row wid100" style="grid-template-columns:28% 50%">
                    <div class="field-cell">
                        <label for="searchInput" class="fontDroid">بحث عن</label>
                        <input type="text" id="searchInput" name="searchInput">
                    </div>
                    <div class="field-cell">
                        <input type="radio" id="idTypeChoice1" name="idType" value="idNum">
                        <label for="idTypeChoice1" class="fontDroid">رقم الاثبات</label>

                        <input type="radio" id="idTypeChoice2" name="idType" value="phone">
                        <label for="idTypeChoice2" class="fontDroid">الاسم</label>

                        <input type="radio" id="idTypeChoice3" name="idType" value="mail">
                        <label for="idTypeChoice3" class="fontDroid">جوال</label>
                        <br>
                        <input type="radio" id="cusGrpChoice2" name="cusGrp" value="phone">
                        <label for="cusGrpChoice2" class="fontDroid">عملائي</label>

                        <input type="radio" id="cusGrpChoice3" name="cusGrp" value="mail">
                        <label for="cusGrpChoice3" class="fontDroid">عملاء النظام</label>
                        <br>
                        <button id="" type="submet" form="frmSearchCustomer" class="btn btn-primary" style="padding: 1px 7px;margin-top: 10px;">بحث</button>
                    </div>
                </div>

                <div class="field-row wid100 overflow" style="height: 400px;">
                        <table id="tblReservations" class="wid100">
                                <thead>                          
                                    <tr class="tblHeader2">
                                        <th>الاسم</th>
                                        <th>رقم الإثبات</th>
                                        <th>جوال</th>
                                        <th>ملاحظات</th>
                                        <th>العمليات</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (typeof customers == 'object' && customers){customers.forEach(customer => { %>
                                        <tr class="tbldatarow">
                                            <td data-title="Name"><%=customer.firstName %> <%=customer.secondName %> <%=customer.thirdName %> <%=customer.lastName %> </td>
                                            <td data-title="ID Number"><%=customer.idNum %></td>    
                                            <td data-title="Mobile"><%=customer.phone %></td>
                                            <td data-title="Notes"><%=customer.notes %></td>
                                            <td data-title="Actions">
                                                <a class="btnSelectCustomer" title="اختيار العميل">
                                                    <i class="far fa-hand-point-right" style="cursor: pointer;"></i>
                                                </a>
                                            </td>
                                        </tr>
                                        <% })} %>
                                </tbody>
                                <tfoot>
                                    <!-- <tr>
                                        <td colspan="6" class="pd10">
