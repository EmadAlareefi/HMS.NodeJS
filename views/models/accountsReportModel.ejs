<div id="accountsReportModel" class="modal fade" role="dialog" aria-labelledby=" " aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title fontDroidBold pdr20" id="AddRoomTitle">Template</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <form id="frmTemplate" action="" method="POST">

                        <table id="tblReservations" class="wid100" >
                                
                        
                                                <thead>
                                                    <tr class="tblHeader1">
                                                        <th colspan="4" class="wid100 fontDroid" style="background-color:#2c91b9;color:white;">تقرير الحسابات </th>
                                                    </tr>
                                                    <tr class="tblHeader2">
                                                        <th>رقم الحساب</th>
                                                        <th>اسم الحساب </th>
                                                        <th>العميل </th>
                                                        <th>الدخول </th>
                                                        
                                
                                                    </tr>
                                                </thead>

                                            <tbody>




                                            <% accounts.forEach(accounts => { %>
                                                <tr class="tbldatarow">
                                                    <td data-title="Name"><%=accounts.accountNumber %></td>
                                                    <td data-title="ID Type"><%=accounts.accountName %></td>
                                                    <td data-title="ID Number"><%=accounts.accountNumber %></td>    
                                                    <td data-title="ID Issue Place"><%=accounts.accountName %></td>
                                                </tr>
                                                <% }) %>



                                               
                                </tbody>
                            </table>
                    
                </form>
            </div>
            <div class="modal-footer">
                <button id="" type="button" form="frmTemplate" class="btn btn-primary">طباعة</button>
                <button id="" type="button" class="btn btn-secondary" data-dismiss="modal"> الغاء الأمر</button>
            </div>
        </div>
    </div>
</div>










