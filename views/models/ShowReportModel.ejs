<div id="ShowReportModel" class="modal fade" role="dialog" aria-labelledby=" " aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title fontDroidBold pdr20" id="AddRoomTitle">Template</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                <form id="frmTemplate" action="" method="POST">

                        <table id="tblReservations" class="wid100" >
                                <thead>
                                    <tr class="tblHeader1">
                                        <th colspan="6" class="wid100 fontDroid" style="background-color:#2c91b9;color:white;">إدارة المشتريات</th>
                                    </tr>
                                    <tr class="tblHeader2">
                                        <th>رقم المشتريات</th>
                                        <th>اسم المشتريات</th>
                                        <th>نوع المشتريات</th>
                                        <th>الكمية </th>
                                        <th>سعر القطعة</th>
                                        <th> المجموع</th>
                
                                    </tr>
                                </thead>
                                <tbody>
                                        <% checkIns.forEach(Purchase => { %>
                                            <tr class="tbldatarow">
                                                <td data-title="Name"><%=checkIns.contractNum %></td>
                                                <td data-title="ID Type"><%=checkIns.roomNumber %></td>
                                                <td data-title="ID Number"><%=checkIns.customer %></td>    
                                                <td data-title="ID Issue Place"><%=checkIns.checkIn %></td>
                                                <td data-title="ID Issue Place"><%=checkIns.checkOut %></td>
                                                <td data-title="ID Issue Place"><%=checkIns.finalPrice%></td>
                                            </tr>
                                            <% }) %>



                                            <% customers.forEach(customer => { %>
                                                <tr class="tbldatarow">
                                                    <td data-title="Name"><%=customer.firstName %> <%=customer.secondName %> <%=customer.thirdName %> <%=customer.lastName %> </td>
                                                    <td data-title="ID Type"><%=customer.idType %></td>
                                                    <td data-title="ID Number"><%=customer.idNum %></td>    
                                                    <td data-title="ID Issue Place"><%=customer.issuingPlace %></td>
                                                    <td data-title="Mobile"><%=customer.phone %></td>
                                                    <td data-title="Email"><%=customer.email %></td>
                                                    <td data-title="Actions">
                                                        <a id="MainContent_lstvw_customers_btnView_0" title="View customer" class="ViewIco TblIco" data-href="/Pages/Management/Popups/View/ViewCustomer.aspx?id=3747766&amp;cid=68"
                                                            href="javascript:__doPostBack('ctl00$MainContent$lstvw_customers$ctrl0$btnView','')"></a>
                                                    </td>
                                                </tr>
                                                <% }) %>




                                            <% accounts.forEach(Purchase => { %>
                                                <tr class="tbldatarow">
                                                    <td data-title="Name"><%=accounts.accountNumber %></td>
                                                    <td data-title="ID Type"><%=accounts.accountName %></td>
                                                    <td data-title="ID Number"><%=accounts.accountNumber %></td>    
                                                    <td data-title="ID Issue Place"><%=accounts.accountName %></td>
                                                </tr>
                                                <% }) %>



                                                <% services.forEach(service => { %>
                                                    <tr class="tbldatarow">
                                                        <td data-title="Name"><%=service.serviceNumber %></td>
                                                        <td data-title="ID Type"><%=service.serviceName %></td>
                                                        <td data-title="ID Number"><%=service.serviceType %></td>    
                                                        <td data-title="ID Issue Place"><%=service.servicePrice %></td>
                                                        <!-- <td data-title="Actions">
                                                            <a id="MainContent_lstvw_customers_btnView_0" title="View customer" class="ViewIco TblIco" data-href="/Pages/Management/Popups/View/Viewservice.aspx?id=3747766&amp;cid=68"
                                                                href="javascript:__doPostBack('ctl00$MainContent$lstvw_customers$ctrl0$btnView','')"></a>
                                                        </td> -->
                                                    </tr>
                                                    <% }) %>




                                        <% Purchases.forEach(Purchase => { %>
                                            <tr class="tbldatarow">
                                                <td data-title="Name"><%=Purchase.PurchaseNumber %></td>
                                                <td data-title="ID Type"><%=Purchase.PurchaseName %></td>
                                                <td data-title="ID Number"><%=Purchase.PurchaseType %></td>    
                                                <td data-title="ID Issue Place"><%=Purchase.Amount %></td>
                                                <td data-title="ID Issue Place"><%=Purchase.PurchasePrice %></td>
                                                <td data-title="ID Issue Place"><%=Purchase.PurchasePrice*Purchase.Amount%></td>
                                            </tr>
                                            <% }) %>



                                            <% rooms.forEach(Purchase => { %>
                                                <tr class="tbldatarow">
                                                    <td data-title="Name"><%=rooms.roomNumber %></td>
                                                    <td data-title="ID Type"><%=rooms.floor %></td>
                                                    <td data-title="ID Number"><%=rooms.roomType %></td>    
                                                    <td data-title="ID Issue Place"><%=rooms.singleBeds%></td>
                                                    <td data-title="ID Issue Place"><%=rooms.doubleBeds%></td>
                                                    <td data-title="ID Issue Place"><%=rooms.dailyPrice %></td>
                                                    <td data-title="ID Issue Place"><%=rooms.peakPrice %></td>
                                                </tr>
                                                <% }) %>
                                </tbody>
                            </table>
                    
                </form>
            </div>
            <div class="modal-footer">
                <button id="" type="submet" form="frmTemplate" class="btn btn-primary">طباعة</button>
                <button id="" type="button" class="btn btn-secondary" data-dismiss="modal"> الغاء الأمر</button>
            </div>
        </div>
    </div>
</div>